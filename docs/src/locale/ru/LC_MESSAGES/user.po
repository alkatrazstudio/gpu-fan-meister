"pts[pts.length-2].y += 10\n"
"$.dev.curvePoints = pts\n"
"\n"
"// задать новый набор точек\n"
"$.dev.curvePoints = [\n"
"    {x: 0, y: 0},\n"
"    {x: 60, y: 0},\n"
"    {x: 100, y: 50}\n"
"]\n"
"\n"
"// попытка задать некорректный набор точек\n"
"$.dev.curvePoints = [\n"
"    {x: 20, y: 30},\n"
"    {x: 50, bogus: 123},\n"
"    {x: 100, y: -10}\n"
"]\n"
"$.dev.curvePoints // [{x: 0, y: 30}, {x: 50, y: 0}, {x: 100, y: 0}]"

